{
  "name": "cmap-js",
  "version": "0.0.1",
  "description": "Display and compare biological maps (genetic, physical, cytogenetic, genomic, linkage groups, chromosomes, scaffolds).",
  "main": "build/cmap.min.js",
  "module": "build/cmap.mjs",
  "browserslist": [
    "ie 10"
  ],
  "scripts": {
    "lint": "eslint src test",
    "build": "rollup --config",
    "pretest": "npm run build",
    "test": "mocha",
    "coverage": "nyc npm test",
    "watch": "rollup -c -w --environment WATCH:yes",
    "validate-config": "ajv -s cmap-schema.json -d cmap.json",
    "postinstall": "for f in data/*.gz; do gzip -dc $f > `echo $f | sed s/\\.gz//`; done",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncgr/cmap-js.git"
  },
  "author": "Andrew Wilkey <awilkey@iastate.edu>, Alex G Rice <agr@ncgr.org>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ncgr/cmap-js/issues"
  },
  "homepage": "https://github.com/ncgr/cmap-js#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "ajv": "*",
    "ajv-cli": "*",
    "axios": "*",
    "babel-eslint": "*",
    "babel-plugin-array-includes": "*",
    "babel-plugin-transform-react-jsx": "*",
    "babel-polyfill": "*",
    "babel-preset-es2015-rollup": "*",
    "babel-register": "*",
    "babelrc-rollup": "*",
    "chai": "*",
    "cssnano": "*",
    "eslint-config-standard": "*",
    "eslint-config-standard-preact": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-standard": "*",
    "livereload": "*",
    "mdn-polyfills": "*",
    "mithril-query": "*",
    "mocha": "*",
    "npm-run-all": "*",
    "nyc": "*",
    "postcss-cssnext": "*",
    "postcss-nested": "*",
    "postcss-simple-vars": "*",
    "promise-polyfill": "*",
    "query-string": "*",
    "rollup": "*",
    "rollup-plugin-babel": "*",
    "rollup-plugin-commonjs": "*",
    "rollup-plugin-eslint": "*",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-livereload": "*",
    "rollup-plugin-node-builtins": "*",
    "rollup-plugin-node-globals": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-postcss": "*",
    "rollup-plugin-replace": "*",
    "rollup-plugin-serve": "*",
    "rollup-plugin-uglify": "*",
    "rollup-watch": "*"
  },
  "dependencies": {
    "a-csv": "*",
    "core-js": "*",
    "csvtojson": "*",
    "ecma-proposal-math-extensions": "*",
    "eslint": "*",
    "hammerjs": "*",
    "hamsterjs": "*",
    "normalize.css": "*",
    "papaparse": "*",
    "preact": "*",
    "preact-router": "*",
    "pubsub-js": "*",
    "random-words": "*",
    "rbush": "*",
    "rbush-knn": "*",
    "skeleton-css": "*",
    "unfetch": "*"
  },
  "nyc": {
    "exclude": [
      "src/main.js",
      "src/CMAP.js",
      "src/developmentTooling.js",
      "build"
    ]
  }
}
