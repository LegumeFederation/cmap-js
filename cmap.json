{
  "#": "cmap-js example configuration file (json format)",
  "header" : ": bean markers and consensus",
  "attribution" : "data attribution: Galeano, Fernandez 2011",
  "initialView" : [ {
      "source" : "phavu_genes",
      "map" : "phavu.Chr02"
        
